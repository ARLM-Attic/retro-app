doctype html
html
    head
        meta(charset="utf-8")
        meta(name="viewport", content="initial-scale=1, maximum-scale=1, user-scalable=no, width=device-width")
        meta(http-equiv="Content-Security-Policy",
            content="default-src *; " +
            "img-src 'self' * *.gravatar.com *.wp.com *.auth0.com data:; " +
            "font-src 'self' *.auth0.com https://fonts.gstatic.com data:; " +
            "style-src * 'unsafe-inline'; " +
            "script-src * 'unsafe-inline' 'unsafe-eval' https://*.auth0.com; " +
            "connect-src 'self' https://api.rollbar.com http://localhost:* ws://retro:1024 ws://127.0.0.1:8080 ws://reactive-retro.herokuapp.com https://reactive-retro.auth0.com")

        title Reactive Retro
        link(href="css/ionic.app.css",  rel="stylesheet")
        link(href="css/game-icons.css", rel="stylesheet")
        script(src="js/lib.js")
        script(src="https://maps.googleapis.com/maps/api/js")
        script(src="cordova.js")
        script(src="js/app.js")
