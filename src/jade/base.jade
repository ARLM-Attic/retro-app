    body(ng-app="retro", ng-controller="MenuController")
        ion-side-menus(enable-menu-with-back-views="false")
            ion-side-menu-content(drag-content="false")
                ion-nav-bar.bar-royal
                    ion-nav-back-button
                    ion-nav-buttons(side="left")
                        button.button.button-icon.icon.ion-navicon(menu-toggle="left", ng-if="!hideMenu")
                    ion-nav-buttons(side="right")
                        button.button.button-clear(ng-if="actionButton", ng-click="actionButton.callback()") {{actionButton.text}}
                ion-nav-view

            ion-side-menu(side="left")
                header.bar.bar-header.bar-assertive
                    h1.title Menu
                ion-content.has-header
                    ion-list
                        ion-item.icon-container(menu-close, ng-repeat="place in menu", ng-click="doMenuAction(place)")
                            i.game-icon(ng-class="place.icon")
                            span &nbsp;{{place.name}}
                            span.pull-right(ng-if="!coords && place.requiresLocation")
                                i.icon.game-icon.game-icon-extra-gps.icon-larger
